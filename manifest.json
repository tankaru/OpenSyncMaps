  {
    "name": "OpenSyncMaps",
    "version": "0.1",
    "description": "Compare maps side by side, mainly for Japanese OpenStreetMap contributors",
	"homepage_url": "https://github.com/tankaru/OpenSyncMaps",
	"permissions": ["activeTab","declarativeContent", "tabs"],
	"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
	"page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icons/icon128.png"
      }
	},
    "icons": {
        "128": "icons/icon128.png"
    },
    "manifest_version": 2
  }